# Firebase (Vite env vars must start with VITE_)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=
VITE_FIREBASE_MEASUREMENT_ID=

# Optional REST API base URL for future server mode
# Example: https://api.example.com
VITE_API_BASE_URL=

# Optional AI service endpoint that accepts POST /v1/summarize { entry }
# Example: https://ai.example.com
VITE_AI_API_URL=

# Optional proxy server (local dev server) base URL, e.g. http://localhost:5050
VITE_PROXY_URL=

# FTC Events API
# Production server: https://ftc-events.firstinspires.org/v2.0/
# Put your authorization token here (do NOT commit your real token to git)
VITE_FTC_API_URL=https://ftc-events.firstinspires.org/v2.0
VITE_FTC_API_KEY=
